<h1>Reviews New page</h1>

<%= form_for [@user, @review] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title, id: 'title' %>
  <%= f.label :body %>
  <%= f.text_area :body, id: 'body' %>
  <%= f.label :image_path %>
  <%= f.text_field :image_path, id: 'image_path' %>

  <%= f.submit %>

<% end %>
