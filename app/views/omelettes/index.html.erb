<h1>Omelettes</h1>
<% @omelettes.each do |omelette| %>
  <%= omelette.title %>
  <%= omelette.description %>
  <%= omelette.ingredients %>
<% end %>

<% if @omelettes.blank? %>
  <h4>There are no recipes containing the term <%= params[:search] %>.</h4>
<% end %>

<%= link_to "Create a New Review", new_reg_user_review_path(@review),
 class: "button", id: "new-review-link" %>
